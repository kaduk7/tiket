generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model UserTb {
  id        Int      @id @default(autoincrement())
  nama      String
  jenis     String
  hp        String   @unique
  wa        String   @unique
  password  String
  foto      String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

}